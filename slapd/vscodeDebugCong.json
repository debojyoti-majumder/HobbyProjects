    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Slapd",
            "request": "launch",
            "type": "cppdbg",
            "MIMode": "lldb",
            "externalConsole": true,
            "program": "${workspaceRoot}/servers/slapd/slapd",
            "cwd": "${workspaceRoot}",
            "args": ["-d", "1", "-f", "./oss/runtime/sldap.conf"],
        }
    ]
}
